	author = {Haggard, Patrick},
	month = jul,
	year = {1998},
	keywords = {Grasp, Human, Movement, Planning},
	pages = {201--215},
	file = {Haggard - 1998 - Planning of action sequences.pdf:/Users/stephenbutterfill/zotero/storage/3UV6ZGWQ/Haggard - 1998 - Planning of action sequences.pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/RSWQTSPN/science.html:text/html}
}

