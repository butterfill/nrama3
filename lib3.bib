@article{russell_why_2012,
	title = {Why {Do} {Young} {Children} {Hide} by {Closing} {Their} {Eyes}? {Self}-{Visibility} and the {Developing} {Concept} of {Self}},
	volume = {13},
	issn = {1524-8372},
