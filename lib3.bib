	file = {Cambridge Journals Snapshot:/Users/stephenbutterfill/zotero/storage/3P5D5KR5/displayAbstract.html:text/html;Suddendorf and Corballis 2007 - The evolution of foresight.pdf:/Users/stephenbutterfill/zotero/storage/TN2M75ME/Suddendorf and Corballis 2007 - The evolution of foresight.pdf:application/pdf}
}

@article{hubbard_representational_2005,
