	pages = {3017--27; author reply 3028--35},
	file = {PubMed Snapshot:/Users/stephenbutterfill/zotero/storage/SHVTA2UH/16125749.html:text/html}
}

@article{crockford_wild_2012,
	title = {Wild {Chimpanzees} {Inform} {Ignorant} {Group} {Members} of {Danger}},
	volume = {22},
