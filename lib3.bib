@article{noles_persistence_2005,
	title = {The persistence of object file representations},
	volume = {67},
	issn = {0031-5117, 1532-5962},
	url = {http://link.springer.com/article/10.3758/BF03206495},
	doi = {10.3758/BF03206495},
	abstract = {Coherent visual experience of dynamic scenes requires not only that the visual system segment scenes into component objects but that these object representations persist, so that an object can be identified as the same object from an earlier time. Object files (OFs) are visual representations thought to mediate such abilities: OFs lie between lower level sensory processing and higher level recognition, and they track salient objects over time and motion. OFs have traditionally been studied viaobjectspecific preview benefits (OSPBs), in which discriminations of an object’s features are speeded when an earlier preview of those features occurred on the same object, as opposed to on a different object, beyond general displaywide priming. Despite its popularity, many fundamental aspects of the OF framework remain unexplored. For example, although OFs are thought to be involved primarily in online visual processing, we do not know how long such representations persist; previous studies found OSPBs for up to 1,500msec but did not test for longer durations. We explored this issue using a modifiedobject reviewing paradigm and found that robust OSPBs persist for more than five times longer than has previously been tested—for at least 8 sec, and possibly for much longer. Object files may be the “glue” that makes visual experience coherent not just in online moment-by-moment processing, but on the scale of seconds that characterizes our everyday perceptual experiences. These findings also bear on research in infant cognition, where OFs are thought to explain infants’ abilities to track and enumerate small sets of objects over longer durations.},
	language = {en},
	number = {2},
	urldate = {2014-10-20},
	journal = {Perception \& Psychophysics},
	author = {Noles, Nicholaus S. and Scholl, Brian J. and Mitroff, Stephen R.},
	month = feb,
	year = {2005},
	keywords = {Cognitive Psychology},
	pages = {324--334},
	file = {Noles and Scholl and Mitroff 2005 - The persistence of object file representations.pdf:/Users/stephenbutterfill/zotero/storage/9ZJPMUPM/Noles and Scholl and Mitroff 2005 - The persistence of object file representations.pdf:application/pdf;Snapshot:/Users/stephenbutterfill/zotero/storage/A4QQEW6V/BF03206495.html:text/html}
}

