	pages = {26--37},
	file = {Gopnik and Astington - 1988 - Children's Understanding of Representational Chang.pdf:/Users/stephenbutterfill/zotero/storage/USH3HFTG/Gopnik and Astington - 1988 - Children's Understanding of Representational Chang.pdf:application/pdf}
}

@article{suchak_how_2016,
	title = {How chimpanzees cooperate in a competitive world},
	volume = {113},
	issn = {0027-8424, 1091-6490},
	url = {http://0-www.pnas.org.pugwash.lib.warwick.ac.uk/content/113/36/10215},
	doi = {10.1073/pnas.1611826113},
	abstract = {Our species is routinely depicted as unique in its ability to achieve cooperation, whereas our closest relative, the chimpanzee (Pan troglodytes), is often characterized as overly competitive. Human cooperation is assisted by the cost attached to competitive tendencies through enforcement mechanisms, such as punishment and partner choice. To examine if chimpanzees possess the same ability to mitigate competition, we set up a cooperative task in the presence of the entire group of 11 adults, which required two or three individuals to pull jointly to receive rewards. This open-group set-up provided ample opportunity for competition (e.g., freeloading, displacements) and aggression. Despite this unique set-up and initial competitiveness, cooperation prevailed in the end, being at least five times as common as competition. The chimpanzees performed 3,565 cooperative acts while using a variety of enforcement mechanisms to overcome competition and freeloading, as measured by (attempted) thefts of rewards. These mechanisms included direct protest by the target, third-party punishment in which dominant individuals intervened against freeloaders, and partner choice. There was a marked difference between freeloading and displacement; freeloading tended to elicit withdrawal and third-party interventions, whereas displacements were met with a higher rate of direct retaliation. Humans have shown similar responses in controlled experiments, suggesting shared mechanisms across the primates to mitigate competition for the sake of cooperation.},
	language = {en},
	number = {36},
	urldate = {2016-09-29},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Suchak, Malini and Eppley, Timothy M. and Campbell, Matthew W. and Feldman, Rebecca A. and Quarles, Luke F. and Waal, Frans B. M. de},
	month = sep,
	year = {2016},
	pmid = {27551075},
	keywords = {enforcement, freeloading, Pan troglodytes, partner choice, Punishment},
	pages = {10215--10220},
	file = {Snapshot:/Users/stephenbutterfill/zotero/storage/XKPDR6JP/10215.html:text/html;Suchak et al 2016 - How chimpanzees cooperate in a competitive world.pdf:/Users/stephenbutterfill/zotero/storage/I7I5B542/Suchak et al 2016 - How chimpanzees cooperate in a competitive world.pdf:application/pdf}
}

