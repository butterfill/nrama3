	pages = {1175--1181},
	file = {Santos et al 2006 - Rhesus monkeys, Macaca mulatta, know what others can and cannot hear.pdf:/Users/stephenbutterfill/zotero/storage/ZC44C22B/Santos et al 2006 - Rhesus monkeys, Macaca mulatta, know what others can and cannot hear.pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/46KN6GU4/S0003347206000662.html:text/html}
