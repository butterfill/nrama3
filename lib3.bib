	keywords = {Adult, Color, Female, Functional Laterality, Humans, Male, Models, Neurological, Visual Fields},
	pages = {1097--102},
	file = {Drivonikou et al 2007 - Further evidence that Whorfian effects are stronger in the right visual field.pdf:/Users/stephenbutterfill/zotero/storage/3ICQMXV4/Drivonikou et al 2007 - Further evidence that Whorfian effects are stronger in the right visual field.pdf:application/pdf}
