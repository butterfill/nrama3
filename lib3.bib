	title = {Do {Infants} {Really} {Expect} {Agents} to {Act} {Efficiently}? {A} {Critical} {Test} of the {Rationality} {Principle}},
	issn = {0956-7976, 1467-9280},
	shorttitle = {Do {Infants} {Really} {Expect} {Agents} to {Act} {Efficiently}?},
	url = {http://pss.sagepub.com/content/early/2013/03/06/0956797612457395},
	doi = {10.1177/0956797612457395},
	abstract = {Recent experiments have suggested that infants’ expectations about the actions of agents are guided by a principle of rationality: In particular, infants expect agents to pursue their goals efficiently, expending as little effort as possible. However, these experiments have all presented infants with infrequent or odd actions, which leaves the results open to alternative interpretations and makes it difficult to determine whether infants possess a general expectation of efficiency. We devised a critical test of the rationality principle that did not involve infrequent or odd actions. In two experiments, 16-month-olds watched events in which an agent faced two identical goal objects; although both objects could be reached by typical, everyday actions, one object was physically (Experiment 1) or mentally (Experiment 2) more accessible than the other. In both experiments, infants expected the agent to select the more-accessible object. These results provide new evidence that infants possess a general and robust expectation of efficiency.},
	language = {en},
	urldate = {2015-06-23},
	journal = {Psychological Science},
	author = {Scott, Rose M. and Baillargeon, Renée},
	month = mar,
	year = {2013},
	pmid = {23470355},
	keywords = {Cognitive development, Efficiency, goals, Infant cognition, infant development, Psychological reasoning, Rationality, Social cognition},
	pages = {0956797612457395},
	file = {Scott and Baillargeon 2013 - Do Infants Really Expect Agents to Act Efficiently.pdf:/Users/stephenbutterfill/zotero/storage/PVVEJB77/Scott and Baillargeon 2013 - Do Infants Really Expect Agents to Act Efficiently.pdf:application/pdf}
}

@article{mlot_fire_2011,
