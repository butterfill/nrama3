	doi = {10.1016/j.jecp.2008.08.003},
	abstract = {We comment on Goldstein, Davidoff, and Roberson's replication and extension (this issue of Journal of Experimental Child Psychology) of our study of the effect of toddlers' color term knowledge on their categorical perception (CP) of color (Journal of Experimental Child Psychology, 90, 114-141 [2005]). First, we discuss how best to assess color term knowledge when concerned with the effects of language on color CP. A reanalysis of our data indicates that even toddlers who do not know the terms for the relevant focal colors still show CP. Second, we comment on Goldstein and colleagues' finding of blue-purple CP, as we did, but not of blue-green CP in Himba toddlers. We present contrasting data from Wright (unpublished PhD thesis, University of Surrey, 2006) that demonstrates blue-green CP in Himba toddlers. Finally, we discuss the limitations of the approach taken by all of these investigations and discuss theoretical accounts of the origin and nature of color CP.},
	urldate = {2008-10-03},
	journal = {Journal of Experimental Child Psychology},
	author = {Franklin, Anna and Wright, Oliver and Davies, Ian R.L.},
	keywords = {Categorical perception, Color perception, Color terms, Toddlers},
	file = {ScienceDirect Full Text PDF:/Users/stephenbutterfill/zotero/storage/PE69RMUC/Franklin et al - What can we learn from toddlers about categorical .pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/NTFTJKVF/science.html:text/html}
}

@article{senju_spontaneous_2012,
	title = {Spontaneous theory of mind and its absence in autism spectrum disorders},
	volume = {18},
	issn = {1073-8584},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3796729/},
	doi = {10.1177/1073858410397208},
	abstract = {Theory of mind, the cognitive capacity to infer others’ mental states, is crucial for the development of social communication. The impairment of theory of mind may relate to autism spectrum disorder (ASD), which is characterised by profound difficulties in social interaction and communication. In the current article, I summarize recent updates in theory of mind research utilizing the spontaneous false belief test, which assesses participants’ spontaneous tendency to attribute belief status to others. These studies reveal that young infants pass the spontaneous false belief test well before they can pass the same task when explicitly asked to answer. By contrast, high-functioning adults with ASD, who can easily pass the false belief task when explicitly asked to, do not show spontaneous false belief attribution. These findings suggest that the capacity for theory of mind develops much earlier than was previously thought, and the absence of spontaneous theory of mind may relate to impairment in social interaction and communication found in ASD.},
	number = {2},
	urldate = {2015-04-18},
	journal = {The Neuroscientist : a review journal bringing neurobiology, neurology and psychiatry},
	author = {Senju, Atsushi},
	month = apr,
	year = {2012},
	pmid = {21609942},
	pmcid = {PMC3796729},
	pages = {108--113},
	file = {Senju 2012 - Spontaneous theory of mind and its absence in autism spectrum disorders.pdf:/Users/stephenbutterfill/zotero/storage/SV7QPMX6/Senju 2012 - Spontaneous theory of mind and its absence in autism spectrum disorders.pdf:application/pdf}
}

