	pages = {1243091},
	file = {Heyes and Frith 2014 - The cultural evolution of mind reading.pdf:/Users/stephenbutterfill/zotero/storage/BSRSDUP8/Heyes and Frith 2014 - The cultural evolution of mind reading.pdf:application/pdf}
}

