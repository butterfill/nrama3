	pages = {479--500},
	file = {ScienceDirect Full Text PDF:/Users/stephenbutterfill/zotero/storage/DCEC6C2N/Saffran et al - 2008 - Grammatical pattern learning by human infants and .pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/67S7SZ64/science.html:text/html}
}

@article{freyd_representational_1984,
	title = {Representational momentum},
	volume = {10},
	copyright = {(c) 2012 APA, all rights reserved},
	issn = {1939-1285(Electronic);0278-7393(Print)},
	doi = {10.1037/0278-7393.10.1.126},
	abstract = {Measured changes in the mental representation of a visually presented pattern induced by a prior sequence of static displays. In Exp I, 16 undergraduates were shown a static rectangle at 3 orientations along a possible path of rotation with each orientation being separated temporally and having an interstimulus interval (ISI) of 250 msec. Ss were instructed to remember the 3rd orientation in the sequence and were then presented with a rectangle at a 4th orientation that was either the same as, or different from, the 3rd. There were 2 ways the orientation could be different: It could be a small rotation in the same direction as the implicit motion, or an equally small rotation in the opposite direction. Ss found it harder to detect differences in the direction of implicit motion, suggesting that their memory for the 3rd orientation had been distorted along that direction. In Exp II (10 undergraduates), it was found that this effect disappeared when the ordering of the 1st 2 orientations was reversed so that there was no longer a consistent path of implicit motion. Exp III (20 college students and employees) showed that when there is a consistent path of implicit motion, the effect can still be found up to ISIs of 500 msec although the effect weakens with longer ISIs. Results are interpreted as evidence for a mental analogue to the momentum of a moving physical object. (14 ref)},
	number = {1},
	journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
	author = {Freyd, Jennifer J. and Finke, Ronald A.},
	year = {1984},
	keywords = {*Form and Shape Perception, *Memory, *Recognition (Learning), *Spatial Imagery, spatial perception},
