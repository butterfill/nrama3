	pages = {87--92},
	file = {Hattori and Kuroshima and Fujita 2009 - Tufted capuchin monkeys (Cebus apella.pdf:/Users/stephenbutterfill/zotero/storage/7CJ9M7FX/Hattori and Kuroshima and Fujita 2009 - Tufted capuchin monkeys (Cebus apella.pdf:application/pdf}
