@article{mcneill_inferentialism_2014,
	title = {Inferentialism and our knowledge of others’ minds},
	issn = {0031-8116, 1573-0883},
	url = {http://link.springer.com/article/10.1007/s11098-014-0358-0},
	doi = {10.1007/s11098-014-0358-0},
	abstract = {Our knowledge of each others’ mental features is sometimes epistemically basic or non-inferential. The alternative to this claim is Inferentialism, the view that such knowledge is always epistemically inferential. Here, I argue that Inferentialism is not plausible. My argument takes the form of an inference to the best explanation. Given the nature of the task involved in recognizing what mental features others have on particular occasions, and our capacity to perform that task, we should not expect always to find good inferential explanations of our knowledge. This conclusion is an epistemological one. The motivation for it is independent of metaphysical concerns about functionalism or about the best way to model the cognitive architecture which produces our knowledge of others’ minds. Given this it is compatible both with the truth of functionalism and theory–theory construed as a cognitive model.},
	language = {en},
	urldate = {2014-09-29},
	journal = {Philosophical Studies},
	author = {McNeill, William E. S.},
	month = aug,
	year = {2014},
	keywords = {Artificial intelligence, Epistemology, Ethics, Inference, Metaphysics, neural networks, Other minds, Philosophy, Philosophy of Language, Philosophy of Mind, Philosophy of Science, Recognition},
	pages = {1--20},
