@article{wolpert_unifying_2003,
	title = {A {Unifying} {Computational} {Framework} for {Motor} {Control} and {Social} {Interaction}},
	volume = {358},
	issn = {09628436},
	url = {http://www.jstor.org/stable/3558137},
	number = {1431},
	urldate = {2008-08-11},
	journal = {Philosophical Transactions: Biological Sciences},
	author = {Wolpert, Daniel M. and Doya, Kenji and Kawato, Mitsuo},
	month = mar,
	year = {2003},
	note = {Recent empirical studies have implicated the use of the motor system during action observation, imitation and social interaction. In this paper, we explore the computational parallels between the processes that occur in motor control and in action observation, imitation, social interaction and theory of mind. In particular, we examine the extent to which motor commands acting on the body can be equated with communicative signals acting on other people and suggest that computational solutions for motor control may have been extended to the domain of social interaction.},
