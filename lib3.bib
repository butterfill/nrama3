	pages = {973--982},
	file = {JSTOR Full Text PDF:/Users/stephenbutterfill/zotero/storage/9FC6BB5H/Pratt and Bryant - 1990 - Young Children Understand That Looking Leads to Kn.pdf:application/pdf;Young Children Understand That Looking Leads to Knowing (So Long as They Are Looking into a Single Barrel):/Users/stephenbutterfill/zotero/storage/JKFNFHX7/1130869.html:text/html}
