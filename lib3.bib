@article{fairhurst_being_2013,
	title = {Being and {Feeling} in {Sync} with an {Adaptive} {Virtual} {Partner}: {Brain} {Mechanisms} {Underlying} {Dynamic} {Cooperativity}},
	volume = {23},
	issn = {1047-3211, 1460-2199},
	shorttitle = {Being and {Feeling} in {Sync} with an {Adaptive} {Virtual} {Partner}},
	url = {http://cercor.oxfordjournals.org/content/23/11/2592},
	doi = {10.1093/cercor/bhs243},
	abstract = {Cooperation is intrinsic to the human ability to work together toward common goals, and depends on sensing and reacting to dynamically changing relationships between coacting partners. Using functional magnetic resonance imaging (fMRI) and a paradigm in which an adaptive pacing signal simulates a virtual partner, we examined the neural substrates underlying dynamic joint action. A single parameter controlled the degree to which the virtual partner adapted its behavior in relation to participant taps, thus simulating varying degrees of cooperativity. Analyses of fMRI data using objective and subjective measures of synchronization quality found the relative balance of activity in two distinct neural networks to depend on the degree of the virtual partner's adaptivity. At lower degrees of adaptivity, when the virtual partner was easier to synchronize with, cortical midline structures were activated in conjunction with premotor areas, suggesting a link between the action and socio-affective components of cooperation. By contrast, right lateral prefrontal areas associated with central executive control processes were recruited during more cognitively challenging interactions while synchronizing with an overly adaptive virtual partner. Together, the reduced adaptive sensorimotor synchronization paradigm and pattern of results illuminate neural mechanisms that may underlie the socio-emotional consequences of different degrees of entrainment success.},
	language = {en},
	number = {11},
	urldate = {2015-07-01},
	journal = {Cerebral Cortex},
	author = {Fairhurst, Merle T. and Janata, Petr and Keller, Peter E.},
	month = nov,
	year = {2013},
	pmid = {22892422},
	keywords = {cognitive control, Cooperation, Entrainment, fMRI, virtual partner},
	pages = {2592--2600},
	file = {Fairhurst and Janata and Keller 2013 - Being and Feeling in Sync with an Adaptive Virtual Partner.pdf:/Users/stephenbutterfill/zotero/storage/CVDAATUM/Fairhurst and Janata and Keller 2013 - Being and Feeling in Sync with an Adaptive Virtual Partner.pdf:application/pdf;Full Text PDF:/Users/stephenbutterfill/zotero/storage/NK2RRR89/Fairhurst et al. - 2013 - Being and Feeling in Sync with an Adaptive Virtual.pdf:application/pdf}
