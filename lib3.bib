	journal = {Experimental Brain Research},
	author = {Atmaca, Silke and Sebanz, Natalie and Knoblich, Günther},
	month = jun,
	year = {2011},
	keywords = {action anticipation, Joint action, Neurology, Neurosciences, Shared representations, Task sharing},
	pages = {371--385},
	file = {Atmaca and Sebanz and Knoblich 2011 - The joint flanker effect.pdf:/Users/stephenbutterfill/zotero/storage/ZPD3RSM9/Atmaca and Sebanz and Knoblich 2011 - The joint flanker effect.pdf:application/pdf;Snapshot:/Users/stephenbutterfill/zotero/storage/HIJV2J6I/s00221-011-2709-9.html:text/html}
