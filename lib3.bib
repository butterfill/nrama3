	journal = {Developmental Science},
	author = {Newcombe, Nora S. and Balcomb, Frances and Ferrara, Katrina and Hansen, Melissa and Koski, Jessica},
	month = sep,
	year = {2014},
	pages = {743--756},
	file = {Newcombe et al 2014 - Two rooms, two representations.pdf:/Users/stephenbutterfill/zotero/storage/HSVEJ2EX/Newcombe et al 2014 - Two rooms, two representations.pdf:application/pdf}
}

