@article{herbranson_artificial_????,
	title = {Artificial grammar learning in pigeons},
	volume = {36},
	issn = {0090-4996, 1532-5830},
	url = {http://link.springer.com/article/10.3758/LB.36.2.116},
	doi = {10.3758/LB.36.2.116},
	abstract = {In two experiments, we developed a new methodology for studying complex stimulus control by spatial sequences of letters generated by artificial grammars. An artificial grammar is a system of rules that defines which letter sequences or strings are “grammatical.” In Experiment 1, pigeons learned to respond differently to strings conforming to a grammar versus strings that were nongrammatical distortions. Several different criteria all suggested that performance was controlled both by some short chunks of strings shared between reinforced training strings and novel transfer strings and by more complex sequential regularities. In Experiment 2, pigeons quickly and accurately learned to respond differently to strings conforming to one or the other of two different artificial grammars. As in Experiment 1, performance was controlled both by some short chunks and by more complex sequential regularities. The results are interpreted in terms of family resemblance and pose new goals for theories of complex stimulus control.},
