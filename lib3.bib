	url = {http://link.springer.com/article/10.1007/s13164-016-0299-y},
	doi = {10.1007/s13164-016-0299-y},
	abstract = {Enactivists often defend the following two claims: (a.) Successful interactions are not driven and explained by the interactors’ ability to mindread (i.e. the ability to attribute beliefs and desires to other agents). And (b.) the mechanisms enabling 2 n d personal social cognition and those enabling 3 r d personal social cognition are distinct. In this paper, I argue that both of these claims are false. With regard to (a.) I argue that enactivists fail to provide a plausible alternative to traditional accounts of social cognition in interaction. I examine and reject Hanne De Jaegher’s view according to which interaction is “constitutive” for social interaction. Furthermore, I critically discuss Shaun Gallagher’s and Daniel Hutto’s views according to which social interactions are exclusively driven by low level cognitive mechanisms such as “gaze following” and “emotion detection”. Concerning (b.), I rely on data from so called “spontaneous response” false belief tasks to show that interactive and observational paradigms require the same “social-cognitive” interpretation.},
	language = {en},
	urldate = {2016-02-03},
	journal = {Review of Philosophy and Psychology},
	author = {Schönherr, Julius},
	month = jan,
	year = {2016},
	keywords = {Cognitive Psychology, developmental psychology, Epistemology, Neurosciences, Philosophy of Mind, Philosophy of Science},
	pages = {1--18},
	file = {Schönherr 2016 - What’s so Special About Interaction in Social Cognition.pdf:/Users/stephenbutterfill/zotero/storage/PJFMFXXQ/Schönherr 2016 - What’s so Special About Interaction in Social Cognition.pdf:application/pdf}
}

