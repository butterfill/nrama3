	author = {Miall, R.C. and Stanley, J. and Todhunter, S. and Levick, C. and Lindo, S. and Miall, J.D.},
	year = {2006},
	keywords = {Forward model, Motor planning, Prediction, Reaction Time, Visual discrimination},
