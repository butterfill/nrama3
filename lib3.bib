@article{clayton_social_2007,
	title = {Social cognition by food-caching corvids. {The} western scrub-jay as a natural psychologist},
	volume = {362},
	url = {http://dx.doi.org/10.1098/rstb.2006.1992},
	doi = {10.1098/rstb.2006.1992},
	abstract = {Food-caching corvids hide food, but such caches are susceptible to pilfering by other individuals. Consequently, the birds use several counter strategies to protect their caches from theft, e.g. hiding most of them out of sight. When observed by potential pilferers at the time of caching, experienced jays that have been thieves themselves, take further protective action. Once the potential pilferers have left, they move caches those birds have seen, re-hiding them in new places. Naive birds that had no thieving experience do not do so. By focusing on the counter strategies of the cacher when previously observed by a potential pilferer, these results raise the intriguing possibility that re-caching is based on a form of mental attribution, namely the simulation of another bird's viewpoint. Furthermore, the jays also keep track of the observer which was watching when they cached and take protective action accordingly, thus suggesting that they may also be aware of others' knowledge states.},
	number = {1480},
	urldate = {2008-08-06},
	journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
	author = {Clayton, Nicola S. and Dally, Joanna M. and Emery, Nathan J.},
	month = apr,
	year = {2007},
	pages = {507--522},
	file = {Royal Society PDF:/Users/stephenbutterfill/zotero/storage/T26U5VJN/Clayton et al - 2007 - Social cognition by food-caching corvids The west.pdf:application/pdf;Royal Society Snapshot:/Users/stephenbutterfill/zotero/storage/SCQNEG8C/q4m8526102w052q1.html:text/html}
