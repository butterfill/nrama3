	file = {Journal of Cognitive Neuroscience Snapshot:/Users/stephenbutterfill/zotero/storage/J97VDVPZ/jocn.2008.html:text/html;Tsai et al. - 2008 - Action Co-representation is Tuned to Other Humans.pdf:/Users/stephenbutterfill/zotero/storage/7GTTVH4A/Tsai et al. - 2008 - Action Co-representation is Tuned to Other Humans.pdf:application/pdf}
}

@article{newtson_variation_1979,
	title = {Variation in behavior perception and ability attribution.},
	volume = {37},
	issn = {0022-3514},
	abstract = {In the 1st of 3 studies, 16 undergraduates segmented a videotape of an actor solving 30 problems in an ascending success pattern under instructions to analyze the 1st half of the sequence into fine or large units. Estimates of future performance were lower when the initial part of the series was analyzed more finely. In a 2nd study, 72 undergraduates viewed either an ascending or descending success pattern; the 1st half of the series was presented at fast motion, normal speed, or slow motion. Slow-motion presentation induced finer units of analysis than normal speed and enhanced primacy effects; fast motion induced larger units of analysis and attenuated primacy effects. A 3rd study with 53 undergraduates compared fine- and large-unit analysis induced by instruction and large-unit analysis induced by fast-motion presentation of the entire ascending and descending series. Results are comparable to those of the 1st 2 studies. It is concluded that primacy effects result from a cessation of processing when a point of subjectively sufficient information is reached and that variations in level of analysis set limits on information gain in observation. (18 ref) (PsycINFO Database Record (c) 2007 APA, all rights reserved)},
	number = {10},
	journal = {Journal of Personality and Social Psychology. Vol. 37(10)},
	author = {Newtson, Darren and Rindner, Richard J},
	month = oct,
	year = {1979},
	keywords = {ability, attribution, social perception, stimulus presentation methods, videotapes},
	pages = {1847--1858},
	file = {Cambridge Scientific Abstracts Snapshot:/Users/stephenbutterfill/zotero/storage/NXCVK7V2/view_record.html:text/html;psp_37_10_1847.pdf:/Users/stephenbutterfill/zotero/storage/2RKBU7CG/psp_37_10_1847.pdf:application/pdf}
}

