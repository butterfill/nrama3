@article{guagnano_sharing_2010,
	title = {Sharing a task or sharing space? {On} the effect of the confederate in action coding in a detection task},
	volume = {114},
	issn = {0010-0277},
	shorttitle = {Sharing a task or sharing space?},
	url = {http://www.sciencedirect.com/science/article/pii/S0010027709002601},
	doi = {10.1016/j.cognition.2009.10.008},
	abstract = {Several studies showed a Simon effect when two participants sit close to each other and perform one of the two halves of a two-choice RT task. That is, each participant perform a go-nogo task. A Simon effect emerges, which instead is absent when the same go-nogo tasks are performed individually. Hence the terms were introduced of “social Simon task” and ‘social Simon effect”. As of now, the social Simon effect was observed with social Simon tasks that overtly gave the participants the impression of being performing in collaboration with another person. In the present study we show that the social Simon effect was present also when, in the social Simon task, the two participants did not actually collaborate. It is, however, absent when participants perform the social task outside of each other’s peripersonal space (i.e., outside arm-reach). We argue that the social Simon effect does not necessarily imply a representation of the other’s action, as previously suggested. The presence of an active confederate in peripersonal space might simply provide a reference for coding one’s own action in space.},
	number = {3},
	urldate = {2014-05-02},
	journal = {Cognition},
	author = {Guagnano, Delia and Rusconi, Elena and Umiltà, Carlo Arrigo},
	month = mar,
	year = {2010},
	keywords = {Action representation, Extrapersonal space, peripersonal space, Response priming},
	pages = {348--355},
	file = {Guagnano and Rusconi and Umiltà 2010 - Sharing a task or sharing space.pdf:/Users/stephenbutterfill/zotero/storage/WW2BJ6VF/Guagnano and Rusconi and Umiltà 2010 - Sharing a task or sharing space.pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/GKIJM7RR/S0010027709002601.html:text/html}
}

@article{waytz_causes_2010,
	title = {Causes and consequences of mind perception},
