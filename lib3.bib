	keywords = {Action understanding, autism, Mirror neurons, Motor Cognition, Social cognition},
	pages = {103--113},
	file = {Gallese et al. - 2009 - Motor Cognition and Its Role in the Phylogeny and .pdf:/Users/stephenbutterfill/zotero/storage/ZEAFXBC7/Gallese et al. - 2009 - Motor Cognition and Its Role in the Phylogeny and .pdf:application/pdf;ScienceDirect Snapshot:/Users/stephenbutterfill/zotero/storage/VWSDSZHE/science.html:text/html}
}

